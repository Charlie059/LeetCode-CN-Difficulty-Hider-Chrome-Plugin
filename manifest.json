{
    "manifest_version": 3,
    "name": "LeetCode CN Difficulty Hider",
    "version": "1.0",
    "description": "Hide difficulty levels on LeetCode CN",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
            "matches": ["https://leetcode-cn.com/*", "https://leetcode.cn/*"],
            "js": ["content.js"]
        }
    ]
}
